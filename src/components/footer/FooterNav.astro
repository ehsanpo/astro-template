---
import { getPortfolioData } from "../../utils/data";
const { navigation } = await getPortfolioData();
---

<div>
  <h3 class="text-lg font-semibold mb-4 text-white">Navigation</h3>
  <ul class="space-y-2">
    {
      navigation.footer.map((link) => (
        <li>
          <a href={link.href} class="text-gray-100 hover:text-gray-200 ">
            {link.text}
          </a>
        </li>
      ))
    }
  </ul>
</div>
